   "source": [
    "N = 10\n",
    "m = 77 + 15.0\n",
    "m_con = 0.28\n",
    "m_t = 0.25\n",
    "m_s = 0.1\n",
    "m_su = 0.18\n",
    "\n",
    "def Mas_0(N, m, m_con, m_t, m_s, m_su):\n",
    "    M_cn = N * m\n",
    "    M = M_cn * 1.1/(1 - m_con - m_t - m_s - m_su)\n",
    "    return (M_cn, M)\n",
    "\n",
    "M = Mas_0(N, m, m_con, m_t, m_s, m_su)\n",
    "print(M)\n"
